<script>
          const availableDevices = '<%- JSON.stringify(availableDevices) %>'
          const optionAssets = '<%- JSON.stringify(optionAssets) %>'
          const deviceAssets = '<%- JSON.stringify(deviceAssets) %>'
</script>
<div onclick="updatePageEventListener(availableDevices)" class='col'>
          
                    <%if(devices.length){%>
                    <ul class="nav nav-tabs" id="devicesTab" role="tablist">
                    <% devices.forEach((device)=> { %>
                             
                              <li class="nav-item" role="presentation">
                                        <button class="nav-link" id="home-tab<%=device.id %>" data-bs-toggle="tab" data-bs-target="#home<%=device.id %>" type="button"
                                                  role="tab" aria-controls="home" aria-selected="true"><%=device.id%>:<%=device.name %>
                                                  <img id='deviceImage' name='<%= device.name %>' class='deviceSVG' type='svg' src=""></button>
                            
                                                  <!--  -->
                                                  
                                                 
                              </li>
    
                              <% }) %>
                    </ul>
          <% } %>

          
          <%if(devices.length){%>
          <div  class="tab-content">
                    <% devices.forEach((device)=> { %>
          
                              <div  class="tab-pane" id="home<%=device.id %>" role="tabpanel" aria-labelledby="home-tab">
          
                                        <%-include('updateForm',{device:device}) %>
                              </div>
                              <% }) %>
          
          </div>
          <% } %>
</div>

<script>
          getdeviceListings(optionAssets, deviceAssets)
</script>